<?xml version="1.0" encoding="utf-8"?>
<beauty.louise.com.view.flexbox.UIFlexBoxView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/flex_box_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    app:alignItems="center"
    app:flexDirection="row"
    app:flexWrap="wrap"
    android:paddingBottom="@dimen/margin_I_II"
    android:paddingTop="@dimen/margin_I_II"
    app:dividerDrawableHorizontal="@drawable/divider_white_small"
    app:justifyContent="space_around"
    app:showDividerHorizontal="middle"
    tools:background="#1f0b9dee"/>
