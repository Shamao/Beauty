<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.louise.gank">

    <application
        android:name="com.louise.base.base.BaseAP"
        android:allowBackup="true"
        android:label="@string/gank_app_name"
        android:supportsRtl="true"
        android:theme="@style/gank_AppTheme">
        <activity android:name=".GankSplashActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>

                <action android:name="android.intent.action.MAIN"/>
                <action android:name="android.intent.action.VIEW"/>
            </intent-filter>
        </activity>
        <activity android:name=".GankMainActivity"
        android:theme="@style/gank_AppTheme">
        </activity>
    </application>

</manifest>